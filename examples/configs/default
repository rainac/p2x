# this is a comment

JUXTA_POST binary 2

";" binary 101 ignoreIfStray

"'" postfix 10001

"," binary  400 right

"?" binary  500 right
"!" binary  500 right

"=" binary  500 right

">" binary 600
"<" binary 600

":" binary 900

"+" binary 1000
#"-" binary 1000
"*" binary 1100
"/" binary 1200
"%" binary 1300

"R" binary 2100 right
"L" binary 2200

"a" binary 2021
"b" binary 2022
"c" binary 2023
"d" binary 2024
"e" binary 2025
"f" binary 2026
"l" binary 2027

"r" binary 2051 right
"s" binary 2052 right
"t" binary 2053 right

"p" postfix 2011
"P" postfix 2301

"lowest" unary 2

"ueqc" unary 2023

"q" unary 2041
"sin" unary 2041

"print" unary 700

"." binary 1099

"^" binary 1200

"@" binary 1300

