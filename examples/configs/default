"-"  unary_binary 1000 left 2200
"ub" unary_binary 1000 left 2023
"ubr" unary_binary 1000 right 2200

"Ub" unary_binary 100 left 3000
"uB" unary_binary 3000 left 100


NEWLINE binary 10

"(" paren ")"
"[" paren "]"
"{" paren "}"

