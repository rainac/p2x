<ncdef name="LogLevel">
  <c name="IGNORE" val="0"/>
  <c name="ERROR" val="1"/>
  <c name="WARNING" val="2"/>
  <c name="INFO" val="4"/>
  <c name="DEBUG" val="8"/>
  <c name="CONFIG" val="0x10"/>
  <c name="PARSE" val="0x20"/>
  <c name="SCAN" val="0x40"/>
  <c name="TIMES" val="0x80"/>
  <c name="FILES" val="0x100"/>
  <c name="IO" val="0x200"/>
  <c name="HELP" val="0x400"/>
</ncdef>
